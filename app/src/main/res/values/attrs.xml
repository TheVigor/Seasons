<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RainDropView">
        <attr name="rv_dot_count" format="integer|reference"/>
        <attr name="rv_max_speed" format="integer|reference"/>
        <attr name="rv_min_speed" format="integer|reference"/>
        <attr name="rv_max_length" format="dimension|reference"/>
        <attr name="rv_min_length" format="dimension|reference"/>
        <attr name="rv_water_radius" format="dimension|reference"/>
        <attr name="rv_max_alpha" format="float|reference"/>
        <attr name="rv_alpha_gradient" format="boolean|reference"/>
    </declare-styleable>

    <declare-styleable name="LeavesLoading">
        <attr name="leafSrc" format="reference"/>
        <attr name="fanSrc" format="reference"/>
        <attr name="bgColor" format="color"/>
        <attr name="progress" format="integer"/>
        <attr name="progressColor" format="color"/>
        <attr name="leafFloatSpeed" format="integer"/>
        <attr name="leafRotateSpeed" format="integer"/>
        <attr name="fanRotateSpeed" format="integer"/>
        <attr name="leafNum" format="integer"/>
        <attr name="fanStrokeColor" format="color"/>
    </declare-styleable>

</resources>